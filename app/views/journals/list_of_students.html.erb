<table class="table table-striped text-center" border="1" cellspacing="0" cellpadding="0">
  <thead class="thead-dark">
    <tr>
      <th>
        Students
      </th>
    </tr>
  </thead>
  <tbody>
  <% @students.each do |student| %>
    <tr>
      <td><%= link_to student.full_name, journal_path(id: student.grade_id)  %></td>
    </tr>
  <% end %>
  </tbody>
</table>