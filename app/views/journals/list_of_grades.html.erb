<table class="table table-striped text-center" border="1" cellspacing="0" cellpadding="0">
  <thead class="thead-dark">
    <tr>
      <th>
        Grades
      </th>
    </tr>
  </thead>
  <tbody>
    <% @grades.each do |grade| %>
      <tr>
        <td><%= link_to grade.grade_group, journal_path(id: grade.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
